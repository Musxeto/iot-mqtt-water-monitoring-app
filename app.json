{
  "expo": {
    "name": "Tanzeem ul Tyari Smart Water Monitoring",
    "slug": "tanzeem-ul-tyari",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tanzeem_logo.jpeg",
    "scheme": "tanzeemultyari",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/tanzeem_logo.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0f172a",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false,
      "package": "com.tanzeemultyari.watermonitor"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tanzeem_logo.jpeg",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#0f172a",
          "dark": {
            "image": "./assets/images/tanzeem_logo.jpeg",
            "backgroundColor": "#0f172a"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "80e095ee-1790-48b6-b84b-fd23675b9a8c"
      }
    }
  }
}
